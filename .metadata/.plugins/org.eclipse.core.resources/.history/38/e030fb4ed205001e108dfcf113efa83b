public interface UserService {
	//insert into T_USER
	void join(UserDTO userDTO);
	
	//select from T_USER where id = ?
	UserDTO getUser(int id, String password);
	
	//아이디 체크
	UserDTO idCheck(int id);

	//로그인
	UserDTO login();
	
}
